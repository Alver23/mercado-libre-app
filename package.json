{
  "name": "mercado-libre-app",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=true npm run build",
    "build": "gulp server:build",
    "clean": "rimraf .next dist build",
    "ci": "npm run lint && npm run test && npm run sonarqube",
    "dev": "DEBUG=mercado-libre-app:* gulp server:dev",
    "dev:debug": "NODE_OPTIONS='--inspect' npm run dev",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "env:dev": "cat ./config/environments/.env ./config/environments/.env.development > .env",
    "env:prod": "cat ./config/environments/.env ./config/environments/.env.production > .env",
    "lint": "eslint --ext .ts,.tsx ./src --ignore-path .eslintignore",
    "lint:fix": "npm run lint -- --fix",
    "lint:report": "npm run lint -- -f html -o reports/lint.html",
    "prettier": "prettier --config .prettierrc --ignore-path ./.prettierignore --write src/**/*.{ts, tsx}",
    "release": "standard-version",
    "sonarqube": "gulp sonarqube",
    "stylelint": "stylelint './src/client/**/*.scss'",
    "stylelint:fix": "npm run stylelint -- --fix",
    "start": "cross-env NODE_ENV=production node build/server/index.js",
    "test": "jest --logHeapUsage",
    "test:watch": "npm test -- --verbose --watch"
  },
  "dependencies": {
    "@hapi/boom": "9.1.2",
    "@next/bundle-analyzer": "10.1.3",
    "axios": "1.6.4",
    "class-transformer": "0.4.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "debug": "2.6.9",
    "dotenv": "8.2.0",
    "es6-shim": "0.35.6",
    "express": "4.17.1",
    "express-healthcheck": "0.1.0",
    "express-rate-limit": "5.2.6",
    "helmet": "4.4.1",
    "http-status-codes": "2.1.4",
    "method-override": "3.0.0",
    "module-alias": "2.2.2",
    "newrelic": "7.3.1",
    "next": "10.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reflect-metadata": "0.1.13",
    "sass": "1.32.8"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "7.13.15",
    "@babel/preset-react": "7.13.13",
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "@jest-mock/express": "1.4.1",
    "@testing-library/react-hooks": "5.1.2",
    "@types/enzyme": "3.10.8",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.37",
    "@types/react": "17.0.3",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.1",
    "autoprefixer": "10.2.5",
    "babel-jest": "26.6.3",
    "coveralls": "3.1.0",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "7.24.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "gulp": "4.0.2",
    "gulp-nodemon": "2.5.0",
    "gulp-shell": "0.8.0",
    "gulp-typescript": "6.0.0-alpha.1",
    "husky": "4.3.8",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "jest-html-reporters": "2.1.3",
    "jest-sonar-reporter": "2.0.0",
    "lint-staged": "10.5.4",
    "postcss": "8.2.10",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.2.1",
    "react-test-renderer": "17.0.2",
    "rimraf": "3.0.2",
    "sonarqube-scanner": "2.8.0",
    "standard-version": "9.2.0",
    "stylelint": "13.12.0",
    "stylelint-config-standard": "21.0.0",
    "supertest": "6.1.3",
    "tailwindcss": "2.1.1",
    "ts-node": "9.1.1",
    "typescript": "4.2.4",
    "webpack-bundle-analyzer": "4.4.1"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "_moduleAliases": {
    "@alversoft": "build"
  }
}
